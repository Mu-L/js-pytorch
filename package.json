{
  "name": "js-pytorch",
  "version": "0.2.2",
  "description": "A JavaScript library like PyTorch, built from scratch.",
  "type": "module",
  "directories": {
    "test": "tests"
  },
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "pkgroll && copyfiles -u 1 ./dist/*.mjs ./assets/demo",
    "check": "tsc -p ./tsconfig.strict.json --noEmit",
    "watch": "pkgroll --watch",
    "prepack": "npm run build",
    "test": "tsx ./tests/test.ts"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "deep-learning",
    "machine-learning",
    "PyTorch"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eduardoleao052/js-torch.git"
  },
  "author": "Eduardo Leitao da Cunha Opice Leao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eduardoleao052/js-torch/issues"
  },
  "homepage": "https://github.com/eduardoleao052/js-torch#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
